<h1> New Vehicle </h1>
    <%= form_for(:vehicle) do |f| %>
        <div class="field">
            <%= f.label :make %><br>
            <%= f.text_field :make %>
        </div>
        <div class="field">
            <%= f.label :model %><br>
            <%= f.text_field :model %>
        </div>
        <div class="field">
            <%= f.label :color %><br>
            <%= f.text_field :color %>
        </div>
        <div class="field">
            <%= f.label :doors %><br>
            <%= f.text_field :doors %>
        </div>
        <div class="field">
            <%= f.label :seating %><br>
            <%= f.text_field :seating %>
        </div>
        <div class="field">
            <%= f.label :engine %><br>
            <%= f.text_field :engine %>
        </div>
        <div class="actions">
            <%= f.submit %>
        </div>
    <% end %>